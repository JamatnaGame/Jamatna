// --- Words helpers (restore original click-to-fill behavior) ---
function fillSlot(ch, charId) {
    const slots = Array.from(document.querySelectorAll('#wd-slots .slot'));
    const empty = slots.find(s => !s.innerText);
    if(!empty) return;
    empty.innerText = ch;
    empty.dataset.tid = charId;
    const tile = document.getElementById(charId);
    if(tile){ tile.classList.add('used'); tile.style.pointerEvents='none'; }
}

function fillIntoSlot(index, ch) {
    const slot = document.getElementById('slot-'+index);
    if(!slot || slot.innerText) return;
    slot.innerText = ch;

    // hide one matching visible tile
    const tiles = Array.from(document.querySelectorAll('#wd-pool .letter-tile'));
    const tile = tiles.find(t => !t.classList.contains('used') && t.innerText === ch);
    if(tile) {
        tile.classList.add('used'); tile.style.pointerEvents='none';
        slot.dataset.tid = tile.id;
    }
}

function showWordHint(btn) {
    if(state.gameData.hintsUsed) return;
    state.gameData.hintsUsed = 1;
    const word = (state.gameData.currWord || "");
    if(!word) return;

    // Ø§Ø®ØªØ± Ø­Ø±ÙÙŠÙ† (ØªØ¬Ø§Ù‡Ù„ Ø§Ù„Ù…Ø³Ø§ÙØ§Øª) Ø­ØªÙ‰ Ù„Ø§ ÙŠÙ†ÙƒØ´Ù "ÙØ±Ø§Øº"
    const chars = Array.from(word);
    const idxs = chars.map((ch, i) => ((ch || '').trim() !== '' ? i : null)).filter(i => i !== null);
    if(idxs.length < 2) return;

    let i = idxs[Math.floor(Math.random()*idxs.length)];
    let j = idxs[Math.floor(Math.random()*idxs.length)];
    while(j === i) j = idxs[Math.floor(Math.random()*idxs.length)];

    fillIntoSlot(i, word[i]);
    fillIntoSlot(j, word[j]);

