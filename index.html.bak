    state.activeTurn = (state.activeTurn==='t1') ? 't2' : 't1';
    updateSnUI();
    return;
  }

  playSound('correct');
  // ØµØ­: ÙŠØªØ­Ø±Ùƒ Ø­Ø³Ø¨ Ø§Ù„Ù†Ø±Ø¯ Ø«Ù… ÙŠØ·Ø¨Ù‘Ù‚ Ø«Ø¹Ø¨Ø§Ù†/Ø³Ù„Ù…
  let pos = state.gameData.pos[team] || 1;
  const dice = state.gameData.pendingDice || 1;
  const next = pos + dice;
  // Ù„Ø§Ø²Ù… Ø±Ù‚Ù… Ø¯Ù‚ÙŠÙ‚ Ù„Ù„ÙˆØµÙˆÙ„ Ù„Ù„Ù†Ù‡Ø§ÙŠØ© (100)
  if(next > SN_END){
    // ØªØ¬Ø§ÙˆØ² 100: Ù„Ø§ ÙŠØªØ­Ø±Ùƒ Ø§Ù„ÙØ±ÙŠÙ‚ ÙˆÙŠÙÙ†Ù‚Ù„ Ø§Ù„Ø¯ÙˆØ±
    state.gameData.sCount = (state.gameData.sCount||0) + 1;
    state.activeTurn = (team==='t1') ? 't2' : 't1';
    updateSnUI();
    return;
  }
  pos = next;
  let jumped = false;
  if(LADDERS_MAP[pos]){ pos = LADDERS_MAP[pos]; jumped = true; }
  else if(SNAKES_MAP[pos]){ pos = SNAKES_MAP[pos]; jumped = true; }

  state.gameData.pos[team] = pos;
  state.gameData.sCount = (state.gameData.sCount||0) + 1;

  if(pos >= SN_END){
    // Ø§Ù„ÙÙˆØ²: ØªÙ†ØªÙ‡ÙŠ Ø§Ù„Ù„Ø¹Ø¨Ø© (ØªÙØ­Ø³Ø¨ Ù†Ù‚Ø§Ø· Ø§Ù„Ø¨Ø·ÙˆÙ„Ø© ÙÙŠ endGame)
    endGame('snakes', team);
    return;
  }

  // Ø¨Ø¯Ù‘Ù„ Ø§Ù„Ø¯ÙˆØ±
  state.activeTurn = (team==='t1') ? 't2' : 't1';
  updateSnUI();
}



