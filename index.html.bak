function applyMemoryCount() {
  const desired = Math.min(10, Math.max(4, parseInt(state.gameData.memDesired || 6)));
  const allPool = ["ğŸ","ğŸŒ","ğŸ‡","ğŸ’","ğŸ“","ğŸ¥","ğŸ","ğŸ‰","ğŸš—","âœˆï¸","ğŸš²","ğŸš€","ğŸ¦","ğŸ¯","ğŸ»","ğŸ˜","ğŸ¦’","ğŸ¸","ğŸ¹","ğŸº","ğŸ»","ğŸ¥","â¤ï¸","ğŸ§¡","ğŸ’›","ğŸ’š","ğŸ’™","ğŸ’œ","ğŸ–¤","âŒš","ğŸ“±","ğŸ’»","ğŸ–¥ï¸","ğŸ§ ","âš½","ğŸ€","ğŸ†","ğŸŒ","ğŸ—ºï¸","ğŸ“š","ğŸ§ª","ğŸ”¬","ğŸ§®"];

