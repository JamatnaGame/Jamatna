function renderQr(el, text){
  if(!el) return;
  try{
    el.innerHTML = "";
    // White container + quiet zone
    el.style.backgroundColor = "#ffffff";
    el.style.padding = "14px";
    el.style.borderRadius = "14px";
    el.style.display = "inline-block";
    el.style.boxShadow = "0 18px 40px rgba(0,0,0,.35)";

    const value = String(text || "");
    new QRCode(el, {
      text: value,
      width: 220,
      height: 220,
      colorDark: "#000000",
      colorLight: "#ffffff",
      correctLevel: QRCode.CorrectLevel.H
    });

    const canvas = el.querySelector('canvas');
    const img = el.querySelector('img');
    const target = canvas || img;
    if(target){
      target.style.width = "220px";
      target.style.height = "220px";
      target.style.display = "block";
      target.style.imageRendering = "pixelated";
    }
  }catch(err){
    console.error('QR render failed', err);
    el.innerHTML = '<div style="color:#111;font-weight:700;">ØªØ¹Ø°Ø± Ø¥Ù†Ø´Ø§Ø¡ Ø§Ù„Ø¨Ø§Ø±ÙƒÙˆØ¯</div>';
    el.style.backgroundColor = "#ffffff";
    el.style.padding = "14px";
    el.style.borderRadius = "14px";
  }
}

function makePantoQrUrl(text){
  const encoded = encodeURIComponent(String(text || ""));
  try{
    // Same folder as the current page (works when served via http(s))
    const u = new URL('panto_view.html', window.location.href);
    u.hash = encoded;
    return u.toString();
  }catch(e){
    return 'panto_view.html#' + encoded;
  }
}


