        // --- Treasure ---
        function initTreasure() {
            state.gameData = { t1Lvl: 0, t2Lvl: 0 };
            state.t1.stuck = false; state.t2.stuck = false;
            updateTreasureTurn();
            const mapT1 = document.getElementById('tr-map');
            mapT1.innerHTML = `<div class="path-col" id="tr-col-t2"></div><div class="path-col" id="tr-col-t1"></div>`;
            ['t1', 't2'].forEach(team => {
                const col = document.getElementById('tr-col-'+team);
                let prevTrap = false; 
                for(let i=0; i<6; i++) {
                    let row = document.createElement('div');
                    row.className = 'path-row';
                    row.id = `row-${team}-${i}`;
                    let count = (i===0||i===5) ? 1 : Math.floor(Math.random()*2)+2;
                    let hasTrapInLevel = false;
                    for(let j=0; j<count; j++) {
                        let node = document.createElement('div');
                        node.className = 'node';
                        if(i===0) node.classList.add('active');
                        node.innerText = i+1;
                        node.dataset.lvl = i;
                        let isTrap = false;
                        if(i > 1 && i < 5 && !prevTrap && !hasTrapInLevel) {
                            if(Math.random() < 0.3) { isTrap = true; prevTrap = true; hasTrapInLevel = true; }
                        }
                        node.dataset.trap = isTrap;
                        node.onclick = () => handleTreasureClick(node, team);
                        row.appendChild(node);
                    }
                    if(!hasTrapInLevel) prevTrap = false;
                    col.appendChild(row);
                }
            });
            showScreen('screen-treasure');
        }
        function handleTreasureClick(node, team) {
            if(state.activeTurn !== team) return;
            let currLvl = (team==='t1') ? state.gameData.t1Lvl : state.gameData.t2Lvl;
            if(parseInt(node.dataset.lvl) !== currLvl) { showCustomModal("<h3>Ø®Ø·Ø£</h3><p>Ø£ÙƒÙ…Ù„ Ø§Ù„Ù…Ø³ØªÙˆÙ‰ Ø§Ù„Ø­Ø§Ù„ÙŠ Ø£ÙˆÙ„Ø§Ù‹</p>"); return; }
            if(node.classList.contains(`done-${team}`)) return;
            state.gameData.pendingNode = node; askQuestionRoutine(team);
        }
        function updateTreasureTurn() {
            if (state.t1.stuck && state.t2.stuck) { declareGrandWinner(); return; }
