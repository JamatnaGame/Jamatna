  // 3) Ù„Ùˆ (Ù†Ø¸Ø±ÙŠØ§Ù‹) Ù…Ø§ ÙƒÙÙ‰ØŒ Ù†Ù…Ù„Ø£ Ù…Ù† Ø£ÙŠ Ø´ÙŠØ¡ Ø¨Ø¯ÙˆÙ† ØªÙƒØ±Ø§Ø± Ù‚Ø¯Ø± Ø§Ù„Ø¥Ù…ÙƒØ§Ù†
  while(arr.length < desired){
    const pick = allPool[Math.floor(Math.random()*allPool.length)];
    if(!arr.includes(pick)) arr.push(pick);
