  let arr = [];
  const base = Array.isArray(state.gameData.memoryBase) ? state.gameData.memoryBase : [];
  for(const it of base){
    if(arr.length >= desired) break;
    if(it != null && !arr.includes(it)) arr.push(it);
  }

  // 2) Ø£ÙƒÙ…Ù„ Ù…Ù† allPool Ù„ÙƒÙ† Ø¨Ø¯ÙˆÙ† ØªÙƒØ±Ø§Ø±
  const candidates = allPool.filter(x => !arr.includes(x));
  while(arr.length < desired && candidates.length){
    const i = Math.floor(Math.random()*candidates.length);
    arr.push(candidates.splice(i,1)[0]);
